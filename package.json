{
  "name": "android-agent",
  "version": "0.1.0",
  "description": "A specialized CLI agent for Android development with Kotlin, Jetpack Compose, and DataWedge",
  "main": "dist/cli.js",
  "bin": {
    "android-agent": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "node --loader ts-node/esm dist/cli.js",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "android",
    "kotlin",
    "jetpack-compose",
    "datawedge",
    "ai-agent",
    "cli",
    "development-tools"
  ],
  "author": "Workshop Participant",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.12",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/inquirer": "^9.0.7",
    "@types/glob": "^8.1.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2"
  },
  "files": [
    "dist/**/*",
    "templates/**/*",
    "README.md",
    "LICENSE"
  ]
}